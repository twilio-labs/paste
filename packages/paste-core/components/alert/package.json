{
  "name": "@twilio-paste/alert",
  "version": "0.1.6",
  "category": "feedback",
  "status": "beta",
  "description": "An alert notifies a user to high-priority or time-sensitive information.",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:dev": "yarn clean && yarn compile:dev",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -cw --environment NODE_ENV:development",
    "prepublishOnly": "yarn build",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@emotion/styled": "^10.0.14",
    "@twilio-paste/box": "^2.1.5",
    "@twilio-paste/button": "^1.1.11",
    "@twilio-paste/icons": "^1.4.2",
    "@twilio-paste/media-object": "^1.1.3",
    "@twilio-paste/types": "^2.2.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-uid": "^2.2.0",
    "styled-system": "^5.1.2"
  },
  "devDependencies": {
    "@twilio-paste/box": "^2.1.5",
    "@twilio-paste/button": "^1.1.11",
    "@twilio-paste/icons": "^1.4.2",
    "@twilio-paste/media-object": "^1.1.3",
    "@twilio-paste/types": "^2.2.3",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "typescript": "^3.5.2"
  }
}
