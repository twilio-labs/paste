    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@twilio-paste/animation-library": "^0.3.1",
    "@twilio-paste/design-tokens": "^6.6.0",
    "@twilio-paste/style-props": "^3.0.6",
    "@twilio-paste/styling-library": "^0.3.3",
