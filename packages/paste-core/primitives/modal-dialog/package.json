    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist",
    "tsc": "tsc"
  },
  "dependencies": {
    "@reach/dialog": "0.11.2"
  },
  "peerDependencies": {
    "@twilio-paste/reakit-library": "^0.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@twilio-paste/reakit-library": "^0.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
