{
  "name": "@twilio-paste/radio-group",
  "version": "2.0.3",
  "category": "user input",
  "status": "production",
  "description": "A radio group is used for selecting a single choice from a list of at least two options",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
