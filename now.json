{
  "name": "paste",
  "scope": "twilio-dsys",
  "version": 2,
  "builds": [{
    "src": "now-build.sh",
    "use": "@now/static-build",
    "config": {
      "distDir": "packages/paste-website/public"
    }
  }],
  "routes": [{
      "src": "/form-elements",
      "status": 301,
      "headers": {
        "Location": "/form-elements/input/"
      }
    },
    {
      "src": "/form/input",
      "status": 301,
      "headers": {
        "Location": "/form-elements/input/"
      }
    },
    {
      "src": "/form-elements/input/",
      "status": 301,
      "headers": {
        "Location": "/components/input/"
      }
    },
    {
      "src": "/form-elements/checkbox/",
      "status": 301,
      "headers": {
        "Location": "/components/checkbox/"
      }
    },
    {
      "src": "/form-elements/select/",
      "status": 301,
      "headers": {
        "Location": "/components/select/"
      }
    },
    {
      "src": "/form-elements/radio-group/",
      "status": 301,
      "headers": {
        "Location": "/components/radio-group/"
      }
    },
    {
      "src": "/form-elements/textarea/",
      "status": 301,
      "headers": {
        "Location": "/components/textarea/"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404/index.html"
    }
  ]
}
