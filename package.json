{
  "name": "paste",
  "private": true,
  "version": "2020.05.19",
  "main": "index.js",
  "author": "Twilio Inc.",
  "license": "MIT",
  "types": "./types/index.d.ts",
  "engines": {
    "node": "^16.13.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prestart": "pnpm prebuild",
    "start": "pnpm build:tokens && concurrently \"pnpm:start:tokens\" \"pnpm:start:storybook\"",
    "start:all": "pnpm build:tokens && concurrently \"pnpm:start:tokens\" \"pnpm:start:storybook:quiet\" \"pnpm:start:website\"",
    "start:storybook": "sb dev -p 9001",
    "start:storybook:quiet": "pnpm start:storybook --quiet",
    "start:tokens": "pnpm nx run @twilio-paste/design-tokens:build:watch",
    "start:website": "pnpm nx run @twilio-paste/website:dev ",
    "start:theme-designer": "pnpm nx run @twilio-paste/theme-designer:dev",
    "start:token-contrast-checker": "pnpm nx run @twilio-paste/token-contrast-checker:start",
    "start:nextjs-template": "pnpm nx run @twilio-paste/nextjs-template:dev",
    "start:contrast-checking": "concurrently \"pnpm:start:tokens\" \"pnpm:start:token-contrast-checker\"",
    "prebuild": "ts-node ./tools/build/pre-build.ts && pnpm packages:check",
    "build": "pnpm prebuild && pnpm nx run-many --target=build --exclude @twilio-paste/website @twilio-paste/theme-designer @twilio-paste/nextjs-template @twilio-paste/token-contrast-checker",
    "build:js": "pnpm prebuild && pnpm nx run-many --target=build:js --exclude @twilio-paste/website @twilio-paste/theme-designer",
    "build:core": "pnpm nx run @twilio-paste/core:build",
    "build:codemods": "pnpm nx run @twilio-paste/codemods:build",
    "build:tokens": "pnpm nx run @twilio-paste/design-tokens:tokens",
    "build:icons": "pnpm nx run @twilio-paste/icons:build",
    "build:storybook": "sb build -c .storybook -o ./docs",
    "build:website": "pnpm nx run @twilio-paste/website:build",
    "build:theme-designer": "pnpm nx run @twilio-paste/theme-designer:build",
    "build:nextjs-template": "pnpm nx run @twilio-paste/nextjs-template:build",
    "build:contrast-checking": "pnpm nx run @twilio-paste/token-contrast-checker:build",
    "pre-push": "concurrently \"pnpm lint\" \"pnpm test\" \"pnpm prettier\" \"pnpm type-check\"",
    "prerelease": "pnpm build && pnpm lint && pnpm test",
    "release": "pnpm changeset publish",
    "release:next": "pnpm lerna publish -m 'chore(release): pre release' --conventional-commits --canary --preid beta --dist-tag next",
    "version": "pnpm changeset version && pnpm i --lockfile-only",
    "sync-internal-peer-dependencies": "ts-node ./tools/build/version.ts",
    "clean": "ts-node ./tools/build/clean-repo.ts && pnpm -r clean && pnpm install",
    "clean:full": "ts-node ./tools/build/clean-repo.ts && pnpm -r clean && rm -rf node_modules/ && pnpm install",
    "clean:core": "pnpm nx run @twilio-paste/core:clean",
    "pre-test": "ts-node ./tools/build/pre-test.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:website": "pnpm cypress run --record",
    "test:website-percy": "percy exec -- pnpm cypress run --record",
    "test:website-gui": "WAIT_ON_TIMEOUT=600000 start-server-and-test 'pnpm start:website' http://localhost:8000 'pnpm cypress open'",
    "test:website-gui-percy": "WAIT_ON_TIMEOUT=600000 start-server-and-test 'pnpm start:website' http://localhost:3000 'percy exec -- pnpm cypress open'",
    "test:storybook": "test-storybook --url http://localhost:9001",
    "start:test:storybook": "start-server-and-test 'NODE_ENV=test yarn start:storybook' http://localhost:9001 'yarn test:storybook'",
    "serve:website": "pnpm nx run @twilio-paste/website:start",
    "package-size-action-build": "pnpm build",
    "prettier": "prettier --list-different '{.storybook,packages}/**/*.{ts,tsx}'",
    "prettier-clean": "prettier --write '{.storybook,packages}/**/*.{ts,tsx}'",
    "lint": "pnpm pre-test && pnpm lint:repo && pnpm lint:core && pnpm lint:website && pnpm lint:remix && pnpm lint:contrast-checker && pnpm lint:nextjs-template && pnpm lint:vscode-intellisense",
    "lint:core": "eslint -c .eslintrc.core.js --ext .tsx,.ts ./packages/{paste-color-contrast-utils,paste-core,paste-customization,paste-design-tokens,paste-icons,paste-libraries,paste-style-props,paste-theme,paste-types,paste-utils}/**/src",
    "lint:website": "eslint -c ./packages/paste-website/.eslintrc --ext .tsx,.ts ./packages/paste-website",
    "lint:remix": "eslint -c ./packages/paste-theme-designer/.eslintrc.json --ext .tsx,.ts ./packages/paste-theme-designer",
    "lint:contrast-checker": "eslint -c ./packages/paste-token-contrast-checker/.eslintrc.json --ext .tsx,.ts ./packages/paste-token-contrast-checker",
    "lint:nextjs-template": "eslint -c ./packages/paste-nextjs-template/.eslintrc.json --ext .tsx,.ts ./packages/paste-nextjs-template",
    "lint:vscode-intellisense": "eslint -c ./apps/vs-code-intellisense/.eslintrc.json --ext .tsx,.ts ./apps/vs-code-intellisense",
    "lint:repo": "eslint -c .eslintrc.repo.js --ext .tsx,.ts .",
    "type-check": "pnpm nx run-many --target=tsc",
    "tsc": "echo 'Did you mean to run yarn type-check?'",
    "chromatic": "chromatic",
    "packages:check": "manypkg check && monopeers check",
    "packages:fix": "manypkg fix && monopeers fix",
    "changeset": "changeset",
    "create:package": "plop create-package",
    "token-usage": "ts-node tools/build/token-usage-detector.ts",
    "nx": "nx"
  },
