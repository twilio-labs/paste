{
  "name": "@twilio-paste/form",
  "version": "1.5.5",
  "category": "user input",
  "status": "beta",
  "description": "All the basic form elements for Twilio Paste",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:dev": "yarn clean && yarn compile:dev",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -c --environment NODE_ENV:development",
    "prepublishOnly": "yarn build",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@styled-system/css": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "@twilio-paste/box": "^2.4.8",
    "@twilio-paste/design-tokens": "^5.2.1",
    "@twilio-paste/flex": "^0.3.27",
    "@twilio-paste/icons": "^2.2.2",
    "@twilio-paste/screen-reader-only": "^1.1.9",
    "@twilio-paste/style-props": "^1.1.9",
    "@twilio-paste/text": "^2.1.9",
    "@twilio-paste/theme": "^3.2.1",
    "@twilio-paste/types": "^3.0.8",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-uid": "^2.2.0",
    "styled-system": "^5.1.2"
  },
  "devDependencies": {
    "@twilio-paste/box": "^2.4.8",
    "@twilio-paste/design-tokens": "^5.2.1",
    "@twilio-paste/flex": "^0.3.27",
    "@twilio-paste/icons": "^2.2.2",
    "@twilio-paste/screen-reader-only": "^1.1.9",
    "@twilio-paste/style-props": "^1.1.9",
    "@twilio-paste/text": "^2.1.9",
    "@twilio-paste/theme": "^3.2.1",
    "@twilio-paste/types": "^3.0.8"
  },
  "dependencies": {
    "react-autosize-textarea": "7.0.0"
  }
}
