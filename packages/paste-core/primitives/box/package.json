    "prop-types": "^15.7.2",
    "react": "^16.8.6 || ^17.0.2 || ^18.0.0",
    "react-dom": "^16.8.6 || ^17.0.2 || ^18.0.0"
  },
  "devDependencies": {
    "@twilio-paste/animation-library": "^0.3.8",
    "@twilio-paste/customization": "^5.0.0",
    "@twilio-paste/design-tokens": "^8.1.0",
    "@twilio-paste/style-props": "^6.1.0",
