{
  "name": "@twilio-paste/typography",
  "version": "1.0.95",
  "category": "typography",
  "status": "alpha",
  "description": "Twilio's Typography component",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:dev": "yarn clean && yarn compile:dev",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -c --environment NODE_ENV:development"
  },
  "dependencies": {
    "@twilio-paste/heading": "^3.0.1",
    "@twilio-paste/list": "^0.2.58",
    "@twilio-paste/paragraph": "^2.0.1"
  },
  "peerDependencies": {
    "@twilio-paste/design-tokens": "^6.4.0",
    "@twilio-paste/style-props": "^1.8.14",
    "@twilio-paste/styling-library": "^0.2.0",
    "@twilio-paste/text": "^2.4.1",
    "@twilio-paste/theme": "^4.1.0",
    "@twilio-paste/types": "^3.0.30",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@twilio-paste/design-tokens": "^6.4.0",
    "@twilio-paste/style-props": "^1.8.14",
    "@twilio-paste/styling-library": "^0.2.0",
    "@twilio-paste/text": "^2.4.1",
    "@twilio-paste/theme": "^4.1.0",
    "@twilio-paste/types": "^3.0.30"
  }
}
