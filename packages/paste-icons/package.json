{
  "name": "@twilio-paste/icons",
  "version": "2.2.0",
  "description": "Twilio's icon library, ready for use and with accessibility considerations",
  "author": "Twilio Inc.",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "scripts": {
    "convert": "node ./tools/cli.js convert",
    "build": "yarn clean && yarn compile",
    "build:dev": "yarn clean && yarn compile:dev",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./esm && rm -rf ./cjs && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -c --environment NODE_ENV:development",
    "prepublishOnly": "yarn build",
    "type-check": "tsc --noEmit",
    "list-icons": "node ./tools/cli.js list-icons",
    "convert-new": "node ./tools/cli.js convert-new"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@styled-system/theme-get": "^5.1.2",
    "@twilio-paste/design-tokens": "^5.2.0",
    "@twilio-paste/style-props": "^1.1.7",
    "@twilio-paste/theme": "^3.1.7",
    "@twilio-paste/types": "^3.0.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-uid": "^2.2.0",
    "styled-system": "^5.1.2"
  },
  "devDependencies": {
    "@twilio-paste/design-tokens": "^5.2.0",
    "@twilio-paste/style-props": "^1.1.7",
    "@twilio-paste/theme": "^3.1.7",
    "@twilio-paste/types": "^3.0.7"
  }
}
