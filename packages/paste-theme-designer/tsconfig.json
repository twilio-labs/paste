{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "emitDeclarationOnly": false,
    "incremental": false,
    "esModuleInterop": true,
    "module": "esnext",
    "jsx": "preserve",
    "allowJs": true
