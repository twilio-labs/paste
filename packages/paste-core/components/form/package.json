{
  "name": "@twilio-paste/form",
  "version": "1.6.3",
  "category": "user input",
  "status": "beta",
  "description": "All the basic form elements for Twilio Paste",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:dev": "yarn clean && yarn compile:dev",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -c --environment NODE_ENV:development",
    "prepublishOnly": "yarn build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react-autosize-textarea": "7.0.0"
  },
  "peerDependencies": {
    "@twilio-paste/box": "^2.8.2",
    "@twilio-paste/design-tokens": "^6.0.0",
    "@twilio-paste/flex": "^0.3.44",
    "@twilio-paste/icons": "^2.6.3",
    "@twilio-paste/screen-reader-only": "^1.1.11",
    "@twilio-paste/style-props": "^1.4.1",
    "@twilio-paste/styling-library": "^0.1.1",
    "@twilio-paste/text": "^2.2.8",
    "@twilio-paste/theme": "^3.2.11",
    "@twilio-paste/types": "^3.0.15",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-uid": "^2.2.0"
  },
  "devDependencies": {
    "@twilio-paste/box": "^2.8.2",
    "@twilio-paste/design-tokens": "^6.0.0",
    "@twilio-paste/flex": "^0.3.44",
    "@twilio-paste/icons": "^2.6.3",
    "@twilio-paste/screen-reader-only": "^1.1.11",
    "@twilio-paste/style-props": "^1.4.1",
    "@twilio-paste/styling-library": "^0.1.1",
    "@twilio-paste/text": "^2.2.8",
    "@twilio-paste/theme": "^3.2.11",
    "@twilio-paste/types": "^3.0.15"
  }
}
