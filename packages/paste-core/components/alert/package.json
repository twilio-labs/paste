    "@twilio-paste/style-props": "^1.8.6",
    "@twilio-paste/styling-library": "^0.1.4",
    "@twilio-paste/theme": "^3.2.27",
    "@twilio-paste/types": "^3.0.28",
    "@twilio-paste/uid-library": "^0.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@twilio-paste/box": "^2.11.6",
