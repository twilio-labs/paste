      "pre-commit": "precise-commits",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "if git-branch-is main; then yarn pre-push; fi"
    }
  },
  "packageManager": "yarn@3.6.3",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11"
  ]
