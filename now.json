{
  "name": "paste",
  "scope": "twilio-dsys",
  "version": 2,
  "builds": [{
    "src": "now-build.sh",
    "use": "@now/static-build",
    "config": {
      "distDir": "packages/paste-website/public"
    }
  }],
  "routes": [{
      "src": "/form",
      "status": 301,
      "headers": {
        "Location": "form/input/"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "404/index.html"
    }
  ]
}
