{
  "name": "@twilio-paste/core",
  "version": "19.0.1",
  "status": "production",
  "description": "Core contains all the packages from the Paste design system.",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main": "src/index.tsx",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/dist",
          "{projectRoot}/*/package.json",
          "{projectRoot}/types"
        ]
      },
      "build:js": {
        "dependsOn": [
          "^build:js"
        ],
        "outputs": [
          "{projectRoot}/dist",
          "{projectRoot}/*/package.json"
        ]
      },
      "build:dev": {
        "dependsOn": [
          "^build:dev"
        ],
        "outputs": [
          "{projectRoot}/dist",
          "{projectRoot}/*/package.json",
          "{projectRoot}/types"
        ]
      },
      "build:types": {
        "dependsOn": [
          "^build:types"
        ],
        "outputs": [
          "{projectRoot}/dist"
        ]
      }
    },
    "includedScripts": [
      "build",
      "build:dev",
      "build:js",
      "build:types"
    ]
  },
  "scripts": {
    "build": "pnpm clean && pnpm generate-packages && pnpm compile && pnpm build:types",
    "build:dev": "pnpm clean && pnpm generate-packages && pnpm compile:dev && pnpm build:types",
    "build:js": "pnpm clean && pnpm generate-packages && pnpm compile",
    "build:types": "./tools/build-types.sh",
    "generate-packages": "ts-node ./tools/generate.ts",
    "clean": "pnpm clean:packages && rm -rf ./dist && rm -rf tsconfig.tsbuildinfo && rm -rf .rpt2_cache",
    "clean:packages": "./tools/clean-packages.sh",
    "compile": "babel ./src/*.tsx --out-dir ./dist --extensions '.tsx' && ts-node ./tools/minify",
    "compile:dev": "babel ./src/*.tsx --out-dir ./dist --extensions '.tsx'"
  },
  "dependencies": {
