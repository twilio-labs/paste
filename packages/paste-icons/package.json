{
  "name": "@twilio-paste/icons",
  "version": "1.7.1",
  "description": "Twilio's icon library, ready for use and with accessibility considerations",
  "author": "Twilio Inc.",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "scripts": {
    "convert": "node ./tools/cli.js convert",
    "build": "yarn clean && yarn compile && yarn props-gen",
    "build:dev": "yarn build",
    "clean": "rm -rf ./esm && rm -rf ./cjs && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -c --environment NODE_ENV:development",
    "prepublishOnly": "yarn build",
    "props-gen": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "type-check": "tsc --noEmit",
    "list-icons": "node ./tools/cli.js list-icons",
    "convert-new": "node ./tools/cli.js convert-new"
  },
  "peerDependencies": {
    "@emotion/styled": "^10.0.10",
    "@twilio-paste/design-tokens": "^4.1.3",
    "@twilio-paste/style-props": "^0.1.8",
    "@twilio-paste/theme-tokens": "^3.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-uid": "^2.2.0",
    "styled-system": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@svgr/babel-plugin-add-jsx-attribute": "^4.0.0",
    "@svgr/babel-plugin-remove-jsx-attribute": "^4.0.3",
    "@svgr/cli": "^4.1.0",
    "@svgr/plugin-jsx": "^4.1.0",
    "@svgr/plugin-svgo": "^4.0.3",
    "@twilio-labs/svg-to-react": "^2.0.0",
    "@twilio-paste/design-tokens": "^4.1.3",
    "@twilio-paste/style-props": "^0.1.8",
    "@twilio-paste/theme-tokens": "^3.0.4",
    "commander": "^2.20.0",
    "lodash.difference": "^4.5.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": " ^4.4.0",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "styled-system": "^5.1.2",
    "typedoc": "^0.16.9",
    "typescript": "3.7.5"
  }
}
