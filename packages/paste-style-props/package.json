{
  "name": "@twilio-paste/style-props",
  "version": "3.2.0",
  "description": "Shared utilities for style properties used in Paste",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && NODE_ENV=production node build.js && tsc",
    "build:js": "NODE_ENV=development node build.js",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist",
    "tsc": "tsc"
  },
  "dependencies": {
    "csstype": "^2.6.10",
    "lodash.camelcase": "^4.3.0"
  },
  "peerDependencies": {
    "@twilio-paste/design-tokens": "^6.6.0",
    "@twilio-paste/styling-library": "^0.3.1",
    "@twilio-paste/theme": "^5.0.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@twilio-paste/design-tokens": "^6.14.1",
    "@twilio-paste/styling-library": "^0.3.5",
    "@twilio-paste/theme": "^5.3.2",
    "prop-types": "^15.7.2"
  }
}
