{
  "name": "@twilio-paste/stack",
  "version": "0.2.4",
  "category": "layout",
  "status": "beta",
  "description": "A horizontal and vertical flow component used to create space between components.",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:dev": "yarn clean && yarn compile:dev",
    "build:props": "typedoc --tsconfig ./tsconfig.json --json ./dist/prop-types.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -c --environment NODE_ENV:development"
  },
  "peerDependencies": {
    "@twilio-paste/box": "^2.12.3",
    "@twilio-paste/design-tokens": "^6.4.1",
    "@twilio-paste/style-props": "^1.8.16",
    "@twilio-paste/styling-library": "^0.2.2",
    "@twilio-paste/theme": "^4.1.2",
    "@twilio-paste/types": "^3.0.31",
    "@twilio-paste/uid-library": "^0.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@twilio-paste/box": "^2.12.3",
    "@twilio-paste/design-tokens": "^6.4.1",
    "@twilio-paste/style-props": "^1.8.16",
    "@twilio-paste/styling-library": "^0.2.2",
    "@twilio-paste/theme": "^4.1.2",
    "@twilio-paste/types": "^3.0.31",
    "@twilio-paste/uid-library": "^0.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
