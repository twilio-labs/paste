{
  "name": "@twilio-paste/typography",
  "version": "1.0.87",
  "category": "typography",
  "status": "alpha",
  "description": "Twilio's Typography component",
  "author": "Twilio Inc.",
  "license": "MIT",
  "main:dev": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:dev": "yarn clean && yarn compile:dev",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo && rm -rf .rpt2_cache",
    "compile": "rollup -c --environment NODE_ENV:production",
    "compile:dev": "rollup -c --environment NODE_ENV:development"
  },
  "dependencies": {
    "@twilio-paste/heading": "^2.1.0",
    "@twilio-paste/list": "^0.2.51",
    "@twilio-paste/paragraph": "^1.1.51"
  },
  "peerDependencies": {
    "@twilio-paste/design-tokens": "^6.3.6",
    "@twilio-paste/style-props": "^1.8.8",
    "@twilio-paste/styling-library": "^0.1.4",
    "@twilio-paste/text": "^2.3.15",
    "@twilio-paste/theme": "^3.3.1",
    "@twilio-paste/types": "^3.0.29",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@twilio-paste/design-tokens": "^6.3.6",
    "@twilio-paste/style-props": "^1.8.8",
    "@twilio-paste/styling-library": "^0.1.4",
    "@twilio-paste/text": "^2.3.15",
