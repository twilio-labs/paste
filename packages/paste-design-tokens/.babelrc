{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "macros",
    ["@babel/proposal-class-properties", { "loose": true }],
    "@babel/proposal-object-rest-spread",
    ["@babel/proposal-private-property-in-object", { "loose": true }],
    "@babel/plugin-proposal-optional-chaining"
  ]
}
