{
  "name": "@twilio-paste/icons",
  "version": "11.0.0",
  "description": "Twilio's icon library, ready for use and with accessibility considerations",
  "author": "Twilio Inc.",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": ["cjs", "esm", "json"],
  "scripts": {
    "build": "pnpm clean && NODE_ENV=production node build.js && pnpm tsc && pnpm list-icons && pnpm postbuild",
    "postbuild": "cp -R ./types/src/* ./cjs && cp -R ./types/src/* ./esm",
    "build:js": "NODE_ENV=development node build.js && pnpm list-icons",
    "clean": "rm -rf ./esm && rm -rf ./cjs && rm -rf ./types",
    "tsc": "tsc",
    "list-icons": "ts-node ./tools/cli.ts list-icons",
    "convert-new": "ts-node ./tools/cli.ts convert-new",
    "convert": "ts-node ./tools/cli.ts convert"
  },
  "peerDependencies": {
    "@twilio-paste/animation-library": "workspace:^",
    "@twilio-paste/box": "workspace:^",
    "@twilio-paste/color-contrast-utils": "workspace:^",
    "@twilio-paste/customization": "workspace:^",
    "@twilio-paste/design-tokens": "workspace:^",
    "@twilio-paste/style-props": "workspace:^",
    "@twilio-paste/styling-library": "workspace:^",
    "@twilio-paste/theme": "workspace:^",
    "@twilio-paste/types": "workspace:^",
    "@twilio-paste/uid-library": "workspace:^",
    "@types/react": "^16.8.6 || ^17.0.2 || ^18.0.27",
    "@types/react-dom": "^16.8.6 || ^17.0.2 || ^18.0.10",
    "prop-types": "^15.7.2",
    "react": "^16.8.6 || ^17.0.2 || ^18.0.0",
    "react-dom": "^16.8.6 || ^17.0.2 || ^18.0.0"
  },
  "devDependencies": {
    "@twilio-paste/animation-library": "workspace:^",
    "@twilio-paste/box": "workspace:^",
    "@twilio-paste/color-contrast-utils": "workspace:^",
    "@twilio-paste/customization": "workspace:^",
    "@twilio-paste/design-tokens": "workspace:^",
    "@twilio-paste/style-props": "workspace:^",
    "@twilio-paste/styling-library": "workspace:^",
    "@twilio-paste/theme": "workspace:^",
    "@twilio-paste/types": "workspace:^",
    "@twilio-paste/uid-library": "workspace:^",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "lodash": "4.17.21",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-node": "^10.0.0"
  }
}
